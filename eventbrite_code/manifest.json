{
	"manifest_version": 2,
	"name": "Eventbrite MF",
	"version": "1.0.0",

	"description": "Find cool experiences near you!",

	"browser_action": {
		"default_icon": "images/eventbrite.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"https://ajax.googleapis.com/"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://maps.googleapis.com https://maps.gstatic.com https://ajax.googleapis.com; object-src 'self'"
}